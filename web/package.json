{
  "name": "web",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "packageManager": "npm@10.2.4",
  "scripts": {
    "prepare:metadata": "node scripts/generate-metadata.js",
    "prepare:tracks": "node scripts/scan-tracks.js",
    "prepare:data": "node scripts/buildData.js",
    "prepare": "npm run prepare:metadata && npm run prepare:tracks && npm run prepare:data",
    "dev": "npm run prepare && vite",
    "build": "npm run prepare && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,css,md}\"",
    "preview": "vite preview",
    "build:analyze": "npm run build -- --mode analyze",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@gsap/react": "^2.1.2",
    "@tanstack/react-virtual": "^3.11.2",
    "d3-drag": "^3.0.0",
    "d3-force": "^3.0.0",
    "d3-selection": "^3.0.0",
    "d3-zoom": "^3.0.0",
    "framer-motion": "^12.27.2",
    "fuse.js": "^7.0.0",
    "gsap": "^3.14.2",
    "lucide-react": "^0.562.0",
    "metaphone": "^2.0.1",
    "natural": "^8.1.0",
    "prop-types": "^15.8.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-force-graph-2d": "^1.29.0",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^7.12.0",
    "react-syntax-highlighter": "^16.1.0",
    "rehype-sanitize": "^6.0.0",
    "remark-gfm": "^4.0.0",
    "wavesurfer.js": "^7.12.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "rollup-plugin-visualizer": "^6.0.5",
    "terser": "^5.46.0",
    "vite": "^7.2.4",
    "vite-plugin-compression": "^0.5.1"
  }
}
